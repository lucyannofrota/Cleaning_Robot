<launch>
  <arg name="node_start_delay" default="2"/>
  
  <node name="space_coverage_planner" pkg="space_coverage" type="space_coverage_node" respawn="false" output="screen" launch-prefix="bash -c 'sleep $(arg node_start_delay); $0 $@' ">
    <param name="x_offset" value="-1"/>
    <param name="y_offset" value="-1"/>
    <param name="goal_dist" value="0.95"/>
    <param name="cell_size" value="0.6"/>
  </node>
</launch>
