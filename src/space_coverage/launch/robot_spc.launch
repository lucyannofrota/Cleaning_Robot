<?xml version="1.0"?>
<launch>
  
  <!-- Space_Coverage_Planner -->
  <node name="space_coverage_planner" pkg="space_coverage" type="space_coverage_node" respawn="false" output="screen" launch-prefix="bash -c 'sleep 0; $0 $@' ">
    <param name="x_offset" value="-1.0000"/>
    <param name="y_offset" value="-13.160000"/>
    <param name="goal_dist" value="0.95"/>
    <param name="cell_size" value="0.6"/>
  </node>

</launch>
